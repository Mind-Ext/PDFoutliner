{
  "name": "pdfoutliner",
  "version": "0.1.2",
  "description": "automatically generate outline for PDF",
  "homepage": "https://github.com/mind-ext/pdfoutliner",
  "license": "AGPL-3.0-only",
  "type": "module",
  "bin": {
    "pdfoutliner": "dist/main.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "esbuild": "^0.20.2",
    "typescript": "^5.4.5",
    "vitest": "^1.4.0"
  },
  "scripts": {
    "build": "cp -r ../mupdf/*.wasm dist && tsc --noEmit && esbuild main.ts --bundle --platform=neutral --packages=external --outdir=dist --target=node20 && cp ../README.md ./",
    "watch": "tsc --noEmit && esbuild main.ts --bundle --platform=neutral --packages=external --outdir=dist --target=node20 --sourcemap=inline --watch",
    "cli": "node dist/main.js",
    "test": "NODE_ENV=test vitest run"
  }
}
